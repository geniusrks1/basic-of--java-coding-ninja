import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
      String str=s.next();
      char ch=str.charAt(0);
      if(ch<='z' && ch>='a')
      System.out.print(0);
      else if(ch<='Z' && ch>='A')
      System.out.print(1);
      else
       System.out.print(-1);

        

    }
}
