
    
    import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int i=0;
		while(i<n){
int space=0;
while(space<i){
	System.out.print(' ');
	space++;
}

int j=0;
while(j<n){
	System.out.print('*');
	j++;
}



			System.out.println();
			i++;
		}

	}
}
