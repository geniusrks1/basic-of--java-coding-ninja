import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
	int n=s.nextInt();
	int i=0;
while(i<n){

int j=0;
int p=1;
int sum=0;
while(j<(2*i+1)){
	if(j%2!=0)
	System.out.print('+');
	else{
	sum+=p;
	System.out.print(p++);
	}

	j++;
}

   System.out.print("="+sum);
	System.out.println();
	i++;
}



	}
}
