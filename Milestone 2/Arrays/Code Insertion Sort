public class Solution {  

    public static void insertionSort(int[] arr) {
        int n=arr.length;
        int i,j;
        for(i=1;i<n;i++){
            int temp=arr[i];
            for(j=i-1; j>=0 && arr[j]>temp;j--){
                arr[j+1]=arr[j];
                
            }
            arr[j+1]=temp;
        }
        
        
        
        
        
        
        
    }

}
